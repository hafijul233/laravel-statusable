    "require": {
        "php": "^7.2|^7.1",
        "illuminate/support": "6.*|5.8.*|5.7.*"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|3.8.*",
        "phpunit/phpunit": "^8.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Omatech\\LaravelStatusable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omatech\\LaravelStatusable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Omatech\\LaravelStatusable\\LaravelStatusableServiceProvider"
            ]
        }
    }
}
